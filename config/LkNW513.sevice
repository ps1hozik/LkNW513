[Unit]
Description=Gunicorn instance to serve LkNW513
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/home/www/LkNW513
Environment="PATH=/home/www/LkNW513/venv/bin"
Environment="DATABASE_URL=postgresql+psycopg2://postgres:<password>@localhost/forms_db" # Заменить <password> на ранее созданный для postgres
ExecStart=/home/www/LkNW513/venv/bin/gunicorn --workers 3 --bind unix:/home/www/LkNW513/LkNW513.sock wsgi:app

[Install]
WantedBy=multi-user.target